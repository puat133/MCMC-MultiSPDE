#!/bin/bash
#SBATCH --time=5-0 #time-requested
#SBATCH --mem-per-cpu=12G # memory
#SBATCH -n 16 #16 process
#SBATCH --cores-per-socket=8
#SBATCH --output=parallel_output_record.out
module load anaconda3

mpiexec -n $SLURM_NTASKS python runNumbaParallel.py --n=64 --n-samples=10000000  --n-layers=3 --sigma-scaling=5e-3 --include-history --enable-beta-feedback
